package com.loiane.estruturadados.vetor.labs;

import java.util.ArrayList;

public class Exer02 {

	public static void main(String[] args) {

		ArrayList<String> arrayList = new ArrayList<String>(5);
		
		arrayList.add("A");
		arrayList.add("B");
		arrayList.add("C");
		
		System.out.println(arrayList.lastIndexOf("Z"));
	}

}
